pipeline {
    agent any
   
        stage('Build') {
            steps {
                dir("C:/ProgramData/Jenkins/.jenkins/workspace/pipeline/my-app") {
                sh 'mvn -B -DskipTests clean package'
                }
            }
        }
     }
    post {
       always {
          junit(
        allowEmptyResults: true,
        testResults: '*/test-reports/.xml'
      )
      }
   } 
}
